# GPU device
/dev/kgsl-2d(.*)			u:object_r:gpu_device:s0
/dev/kgsl-3d0				u:object_r:gpu_device:s0
/dev/msm_rotator			u:object_r:gpu_device:s0

# Blocks
/dev/block/mmcblk0			u:object_r:root_block_device:s0
/dev/block/mmcblk0p13		        u:object_r:boot_block_device:s0
/dev/store/cm-cache			u:object_r:cache_block_device:s0
/dev/store/cm-data			u:object_r:userdata_block_device:s0
/dev/store/cm-system			u:object_r:system_block_device:s0
/dev/store/swap			u:object_r:swap_block_device:s0

